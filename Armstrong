#include <iostream>
#include <string>
using namespace std;

int main() {
    
    int number_input;
    int digit_count = 0;
    int digit_array[10];
    int sum = 0;
    int initial_input;
    
    cout << "Enter number: ";
    cin >> number_input;
    initial_input = number_input; 
    
    while (number_input > 0){
        digit_array[digit_count] = number_input % 10;
        number_input /= 10;
        digit_count++;
    }
    
    for (int i = 0; i < digit_count; i++){
        int result = 1;
        for (int j = 0; j < digit_count; j++){
        result = result * digit_array[i];
        }
        sum = sum + result;
    }
    
    if (sum == initial_input){
        cout << "Result: Armstrong";
    } else {
        cout << "Result: Not Armstrong";
    }
    
    
    
}
