#include <iostream>
using namespace std;

int main(){
    
    int M, N;
    cout << "Enter matrix dimensions (M N): ";
    cin >> M >> N;
    
    int initial_array[100][100], transposed_array[100][100];
    
    cout << "Enter matrix elements: " << endl;
    for (int i = 0; i < M; i++){
        for (int j = 0; j < N; j++){
            cin >> initial_array[i][j];
    }
    }
    //transposer
    for (int i = 0; i < M; i++){
        for (int j = 0; j < N; j++){
            transposed_array[j][i] = initial_array[i][j];
    }
    }
    // output
    cout << "Transposed Matrix: " << endl;
    for (int i = 0; i < N; i++){
        for (int j = 0; j < M; j++){
            cout << transposed_array[i][j] << " ";
    }
    cout << endl;
    }
    
    return 0;

}
