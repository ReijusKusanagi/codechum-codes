#include <iostream>
#include <string>
using namespace std;

// pseeudo code:
// nesting if else, basic

int main(){
    
    int score;
    
    cout << "Enter score: ";
    cin >> score;
    
    if (score >= 90 && score <= 100){
        cout << "Grade: A";
    } else if (score >= 80 && score <= 89){
        cout << "Grade: B";
    } else if (score >= 70 && score <= 79){
        cout << "Grade: C";
    } else if (score >= 60 && score <= 69){
        cout << "Grade: D";
    } else if (score >= 0 && score <= 59){
        cout << "Grade: F";
    } else {
        cout << "Invalid Input. Score must be from 0 to 100.";
    }
    
    return 0;
}
