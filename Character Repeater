#include <iostream>
#include <string>
using namespace std;

int main(){
    
    int number;
    string input;
    
    cout << "Enter a character and a number: ";
    cin >> input >> number;
    
    // cout << input << number << endl;
    cout << "Result: ";
    for (int i = 0; i < number; i++){
         cout << input;
    }
    
    return 0;
}
